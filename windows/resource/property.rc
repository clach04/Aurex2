LANGUAGE_JAPANESE

PROP_PAGE_MISC DIALOG DISCARDABLE  0, 0, 120, PROP_HEIGHT
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "その他"
FONT 10, "ＭＳ ゴシック" /* width 全角で 8 ？ */
BEGIN
    CONTROL     	"奇数アドレス多バイト転送命令を警告（デベロッパ向け）",
			IDC_WARN_ODD, "Button",
			BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 10, 220, 10
