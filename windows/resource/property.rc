			"Button",BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 40, 140, 10

    CONTROL         	"複数起動しない（次回起動時から有効）",IDC_ONLY_A_PROCESS,
			"Button",BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 55, 180, 10

    CONTROL         	"ステータスバーの表示",IDC_ENABLE_STATUS_BAR,
			"Button",BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 70, 140,10
    CONTROL         	"タイトルバーもいらない",IDC_NO_CAPTION,
			"Button",BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 115, 140,10
    CONTROL         	"ISO8859-1を使用する",IDC_ISO8859_1,
			"Button",BS_AUTOCHECKBOX | WS_TABSTOP,
			10, 130, 140,10
END

